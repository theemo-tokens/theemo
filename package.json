{
  "name": "theemo",
  "description": "The yordle powered theme automator",
  "version": "0.0.7",
  "license": "MIT",
  "author": "Thomas Gossmann",
  "homepage": "https://gossi.github.io/theemo/",
  "bugs": "https://github.com/gossi/theemo/issues",
  "repository": "gossi/theemo",
  "main": "dist/index.js",
  "module": "dist/theemo.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && tsdx build --target node",
    "start": "tsdx watch --target node",
    "_test": "tsdx test",
    "lint:js": "eslint --ext ts,js src",
    "lint:types": "tsc --noEmit",
    "lint": "yarn lint:js && yarn lint:types",
    "api": "yarn build && node scripts/make-docs.js",
    "prepublish": "yarn build"
  },
  "bin": {
    "theemo": "bin/theemo.js"
  },
  "dependencies": {
    "color-converter": "1.4.1",
    "commander": "^5.0.0",
    "dotenv": "^8.2.0",
    "esm": "3.2.25",
    "figma-api": "^1.5.5",
    "node-fetch": "^2.6.0",
    "style-dictionary": "^2.8.3"
  },
  "devDependencies": {
    "@clark/eslint-config-typescript": "^1.23.0",
    "@microsoft/api-documenter": "^7.7.15",
    "@microsoft/api-extractor": "^7.7.11",
    "@types/jest": "^24.0.25",
    "@types/node-fetch": "^2.5.4",
    "eslint": "6.8.0",
    "tsdx": "^0.12.3",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  }
}
