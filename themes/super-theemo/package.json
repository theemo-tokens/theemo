{
  "name": "super-theemo-theme",
  "private": true,
  "description": "Super Theemo theme",
  "license": "MIT",
  "keywords": [
    "theemo-theme"
  ],
  "type": "module",
  "theemo": {
    "name": "theemo",
    "colorSchemes": [
      "dark"
    ],
    "file": "dist/theemo.css"
  },
  "scripts": {
    "_clean": "rm -rf build/ dist/ tokens/",
    "sync": "DEV=true theemo sync",
    "build:css": "style-dictionary build",
    "_build:base": "style-dictionary build --config config.base.js",
    "_build:light": "style-dictionary build --config config.light.js",
    "_build:dark": "style-dictionary build --config config.dark.js",
    "_build:website": "pnpm build:website:*",
    "_build:website:base": "style-dictionary build --config config.website.js",
    "_build:website:dark": "style-dictionary build --config config.website.dark.js",
    "generate": "theemo generate",
    "magic": "sync build generate",
    "_clean-magic": "pnpm clean && pnpm magic"
  },
  "devDependencies": {
    "@gossi/config-eslint": "^0.14.0",
    "@gossi/config-prettier": "^0.10.0",
    "@theemo/build": "workspace:*",
    "@theemo/cli": "workspace:*",
    "@theemo/figma": "workspace:*",
    "@theemo/style-dictionary": "workspace:*",
    "@theemo/sync": "workspace:*",
    "@theemo/tokens": "workspace:*",
    "eslint": "^8.57.1",
    "prettier": "^3.5.3",
    "style-dictionary": "^4.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
