{
  "name": "@theemo-playground/ocean-theme",
  "private": true,
  "description": "Sample ocean theemo theme",
  "keywords": [
    "theemo-theme"
  ],
  "type": "module",
  "theemo": {
    "name": "ocean",
    "file": "nested/blue-ocean.css",
    "features": [
      {
        "name": "color-scheme",
        "options": [
          "light",
          "dark"
        ],
        "defaultOption": "light",
        "browserFeature": "color-scheme",
        "behavior": [
          "adaptive",
          "mode"
        ]
      }
    ]
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
