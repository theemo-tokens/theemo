{
  "name": "@theemo-playground/sveltekit",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "preview": "vite preview",
    "_prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/kit": "^2.28.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@theemo/svelte": "workspace:*",
    "@theemo/theme": "workspace:*",
    "@theemo/vite": "workspace:*",
    "@theemo-playground/forest-theme": "workspace:*",
    "@theemo-playground/ocean-theme": "workspace:*",
    "svelte": "^5.39.4",
    "svelte-check": "^4.3.2",
    "typescript": "^5.9.2",
    "vite": "^6.3.6"
  }
}
