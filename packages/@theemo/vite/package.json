{
  "name": "@theemo/vite",
  "version": "0.0.0",
  "description": "Plugin for loading themes with vite",
  "license": "MIT",
  "author": "Thomas Gossmann",
  "homepage": "https://theemo.io",
  "bugs": "https://github.com/theemo-tokens/theemo/issues",
  "repository": "https://github.com/theemo-tokens/theemo",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "design system",
    "theme",
    "design tokens",
    "vite-plugin"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "lint": "concurrently -g 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently -g 'npm:lint:*:fix'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "start": "tsup --watch",
    "_test": "vitest run",
    "_test:dev": "vitest"
  },
  "peerDependencies": {
    "@farmfe/core": ">=1",
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@farmfe/core": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "devDependencies": {
    "@gossi/config-eslint": "^0.13.0",
    "@gossi/config-prettier": "^0.9.1",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "prettier": "^3.3.3",
    "tsup": "8.4.0",
    "typescript": "5.8.3",
    "type-fest": "^4.40.0",
    "vite": "^6.2.6",
    "vitest": "3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
