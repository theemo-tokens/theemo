{
  "name": "@theemo/figma",
  "version": "0.2.2",
  "description": "Theemo Figma Integration",
  "license": "MIT",
  "author": "Thomas Gossmann",
  "homepage": "https://theemo.io",
  "bugs": "https://github.com/theemo-tokens/theemo/issues",
  "repository": "https://github.com/theemo-tokens/theemo",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "design system",
    "theme",
    "design tokens",
    "figma",
    "theemo"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "lint": "concurrently -g 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently -g 'npm:lint:*:fix'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "start": "tsup --watch",
    "test": "vitest run",
    "test:dev": "vitest"
  },
  "dependencies": {
    "@theemo/tokens": "0.2.2",
    "@theemo/sync": "0.2.2",
    "colord": "^2.9.3",
    "figma-api": "^1.11.0",
    "lodash.isempty": "^4.4.0",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@gossi/config-eslint": "^0.12.0",
    "@gossi/config-prettier": "^0.9.0",
    "@types/lodash.isempty": "^4.4.7",
    "@types/lodash.merge": "^4.6.7",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.49.0",
    "prettier": "^3.3.3",
    "tsup": "^8.0.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
