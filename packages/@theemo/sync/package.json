{
  "name": "@theemo/sync",
  "version": "0.2.1",
  "description": "Theemo Sync",
  "license": "MIT",
  "author": "Thomas Gossmann",
  "homepage": "https://theemo.io",
  "bugs": "https://github.com/theemo-tokens/theemo/issues",
  "repository": "https://github.com/theemo-tokens/theemo",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "design system",
    "theme",
    "design tokens"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "lint": "concurrently -g 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently -g 'npm:lint:*:fix'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "start": "tsup --watch",
    "test": "vitest run",
    "test:dev": "vitest"
  },
  "dependencies": {
    "@theemo/tokens": "0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@gossi/config-eslint": "^0.6.0",
    "@gossi/config-prettier": "^0.6.0",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "concurrently": "^8.2.0",
    "eslint": "^8.49.0",
    "prettier": "^3.0.3",
    "tsup": "^8.0.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
