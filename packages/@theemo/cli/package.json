{
  "name": "@theemo/cli",
  "version": "0.2.4",
  "description": "Theemo CLI",
  "license": "MIT",
  "author": "Thomas Gossmann",
  "homepage": "https://theemo.io",
  "bugs": "https://github.com/theemo-tokens/theemo/issues",
  "repository": "https://github.com/theemo-tokens/theemo",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "design system",
    "theme",
    "design tokens"
  ],
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "theemo": "./bin/theemo.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "require": "./dist/cli.cjs",
      "default": "./dist/cli.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "lint": "concurrently -g 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently -g 'npm:lint:*:fix'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "start": "tsup --watch",
    "test": "vitest run",
    "test:dev": "vitest"
  },
  "dependencies": {
    "@theemo/build": "workspace:*",
    "@theemo/sync": "workspace:*",
    "commander": "^13.0.0",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^16.3.1",
    "read-pkg-up": "^11.0.0",
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "@gossi/config-eslint": "^0.13.0",
    "@gossi/config-prettier": "^0.9.1",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.10.15",
    "@types/node": "^22.10.5",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "prettier": "^3.3.3",
    "tsup": "8.3.6",
    "typescript": "5.7.3",
    "vitest": "3.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
