{
  "name": "@theemo/style-dictionary",
  "version": "0.2.2",
  "description": "Definitions for Theemo Sync Tools",
  "license": "MIT",
  "author": "Thomas Gossmann",
  "homepage": "https://theemo.io",
  "bugs": "https://github.com/theemo-tokens/theemo/issues",
  "repository": "https://github.com/theemo-tokens/theemo",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "design system",
    "theme",
    "design tokens",
    "style dictionary",
    "theemo"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "lint": "concurrently -g 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently -g 'npm:lint:*:fix'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "start": "tsup --watch",
    "test": "vitest run",
    "test:dev": "vitest"
  },
  "dependencies": {
    "@theemo/tokens": "0.2.2",
    "@theemo/sync": "0.2.2"
  },
  "peerDependencies": {
    "style-dictionary": ">= 3"
  },
  "devDependencies": {
    "@gossi/config-eslint": "^0.12.0",
    "@gossi/config-prettier": "^0.9.0",
    "@types/jsonpath": "^0.2.0",
    "@types/mock-fs": "^4.13.1",
    "concurrently": "^8.2.0",
    "eslint": "^8.49.0",
    "jsonpath": "1.1.1",
    "mock-fs": "5.2.0",
    "prettier": "^3.3.3",
    "style-dictionary": "4.0.1",
    "tsup": "8.2.4",
    "typescript": "5.5.4",
    "vitest": "2.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
