{
  "name": "@theemo/style-dictionary",
  "version": "0.2.3",
  "description": "Definitions for Theemo Sync Tools",
  "license": "MIT",
  "author": "Thomas Gossmann",
  "homepage": "https://theemo.io",
  "bugs": "https://github.com/theemo-tokens/theemo/issues",
  "repository": "https://github.com/theemo-tokens/theemo",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "design system",
    "theme",
    "design tokens",
    "style dictionary",
    "theemo"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "lint": "concurrently -g 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently -g 'npm:lint:*:fix'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "start": "tsup --watch",
    "test": "vitest run",
    "test:dev": "vitest"
  },
  "dependencies": {
    "@theemo/tokens": "workspace:*",
    "@theemo/sync": "workspace:*"
  },
  "peerDependencies": {
    "style-dictionary": ">= 3"
  },
  "devDependencies": {
    "@gossi/config-eslint": "^0.13.0",
    "@gossi/config-prettier": "^0.9.1",
    "@types/jsonpath": "^0.2.4",
    "@types/mock-fs": "^4.13.4",
    "concurrently": "^9.1.0",
    "eslint": "^8.57.1",
    "jsonpath": "1.1.1",
    "mock-fs": "5.4.1",
    "prettier": "^3.3.3",
    "style-dictionary": "3.9.2",
    "tsup": "8.3.5",
    "typescript": "5.6.3",
    "vitest": "2.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
