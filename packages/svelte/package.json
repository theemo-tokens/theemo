{
  "name": "@theemo/svelte",
  "version": "0.0.0",
  "description": "Theemo integration for Svelte",
  "license": "MIT",
  "author": "Thomas Gossmann",
  "homepage": "https://theemo.io",
  "bugs": "https://github.com/theemo-tokens/theemo/issues",
  "repository": "https://github.com/theemo-tokens/theemo",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "design system",
    "theme",
    "design tokens"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.svelte.d.ts",
      "svelte": "./dist/index.svelte.js"
    },
    "./kit": {
      "types": "./dist/kit.d.ts",
      "default": "./dist/kit.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "lint": "concurrently -g 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently -g 'npm:lint:*:fix'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "start": "tsdown --watch",
    "_test": "vitest run",
    "_test:dev": "vitest"
  },
  "dependencies": {
    "@theemo/theme": "workspace:*",
    "@theemo/vite": "workspace:*"
  },
  "peerDependencies": {
    "svelte": "^5.0.0",
    "@sveltejs/kit": "^2.0.0",
    "vite": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "@sveltejs/kit": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@gossi/config-eslint": "^0.14.0",
    "@gossi/config-prettier": "^0.10.0",
    "@sveltejs/kit": "^2.21.2",
    "@theemo-config/tsconfig": "workspace:*",
    "@theemo-config/tsdown": "workspace:*",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "prettier": "^3.5.3",
    "rollup": "^4.42.0",
    "svelte": "^5.33.18",
    "tsdown": "^0.12.7",
    "typescript": "5.8.3",
    "vite": "^6.3.5",
    "vitest": "3.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
